<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="699.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs112.lab09.controllers.MainController">
   <children>
      <Label fx:id="titleLabel" alignment="CENTER" prefHeight="55.0" prefWidth="765.0" text="Title">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font></Label>
      <Label fx:id="subtitleLabel" alignment="CENTER" prefHeight="52.0" prefWidth="827.0" text="Subtitle">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font></Label>
      <Label fx:id="descriptionLabel" alignment="CENTER" prefHeight="233.0" prefWidth="600.0" text="Description" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font></Label>
      <Pane prefHeight="187.0" prefWidth="600.0">
         <children>
            <ImageView fx:id="imageLabel" fitHeight="187.0" fitWidth="305.0" layoutX="148.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </Pane>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleStartButton" prefHeight="25.0" prefWidth="53.0" text="Start" />
            <Button mnemonicParsing="false" onAction="#handleQuitButton" prefHeight="25.0" prefWidth="54.0" text="Quit" />
         </children>
      </HBox>
   </children>
</VBox>
